// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id Int @id @default(autoincrement())
  name String
  email String
  password String
}

model Products {
  id Int @id @default(autoincrement())
  title String
  description String
  image_url String
  category String
  rating String
  price String
  num_reviews String
  user_id Int
}

model fav {
  id Int @id @default(autoincrement())
  user_id Int
  product_id Int
}
model cart {
  id Int @id @default(autoincrement())
  user_id Int
  product_id Int
}